<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>

<AnchorPane fx:id="anchorPane" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.111" xmlns:fx="http://javafx.com/fxml/1" fx:controller="runners.javafx.controllers.MainMenu">
   <children>
      <VBox alignment="CENTER" layoutX="42.0" layoutY="35.0" maxHeight="300.0" maxWidth="150.0" minHeight="150.0" minWidth="80.0" prefHeight="200.0" prefWidth="100.0" spacing="20.0">
         <children>
            <Button fx:id="btLoadXml" mnemonicParsing="false" onMouseClicked="#handleLoadXmlButtonPressed" text="Load XML" />
            <Button fx:id="btStartGame" disable="true" layoutX="21.0" layoutY="10.0" mnemonicParsing="false" onMouseClicked="#handleStartGameButtonPressed" text="Play Game" />
            <Button fx:id="btQuit" layoutX="21.0" layoutY="37.0" mnemonicParsing="false" onMouseClicked="#handleQuitGameButtonPressed" text="Quit Game" textFill="RED" />
         </children>
      </VBox>
   </children>
</AnchorPane>
