<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<VBox alignment="CENTER" maxHeight="140.0" maxWidth="220.0" minHeight="140.0" minWidth="220.0" spacing="40.0" xmlns="http://javafx.com/javafx/8.0.111" xmlns:fx="http://javafx.com/fxml/1" fx:controller="runners.javafx.controllers.ConfirmBoxController">
   <children>
      <Label fx:id="message" text="Label" textAlignment="CENTER" wrapText="true" />
      <HBox alignment="TOP_CENTER" spacing="50.0">
         <children>
            <Button fx:id="cancelButton" mnemonicParsing="false" onAction="#handleCloseButtonPressed" text="Cancel" />
            <Button fx:id="okButton" mnemonicParsing="false" onAction="#handleCloseButtonPressed" text="OK" />
         </children>
         <padding>
            <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
         </padding>
      </HBox>
   </children>
   <padding>
      <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
   </padding>
</VBox>
